      <JFXButton fx:id="rewards" buttonType="RAISED" layoutX="115.0" layoutY="248.0" onAction="#onRewardsClicked" style="-fx-background-color: #212121;" text="My Galaxy" textFill="#ffffffb2" />
      <JFXButton fx:id="newstudy" buttonType="RAISED" layoutX="103.0" layoutY="339.0" onAction="#onNewClicked" style="-fx-background-color: #212121;" text="New Study Set" textFill="#ffffffb2" />
      <Label fx:id="clock" layoutX="83.0" layoutY="44.0" text="00:00 AM" textFill="WHITE">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label fx:id="date" layoutX="107.0" layoutY="102.0" text="______ __" textFill="WHITE">
