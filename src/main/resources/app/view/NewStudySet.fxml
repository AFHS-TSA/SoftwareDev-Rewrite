<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="baseBorder" prefHeight="250.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.app.controllers.NewStudySetController">
	 <center>
      <AnchorPane style="-fx-background-color: #424242;" BorderPane.alignment="CENTER">
         <children>
            <JFXRadioButton fx:id="self" layoutX="33.0" layoutY="108.0" onAction="#onSelf" text="Self Made Set" textFill="WHITE">
               <toggleGroup>
                  <ToggleGroup fx:id="studyPick" />
               </toggleGroup>
            </JFXRadioButton>
            <JFXRadioButton fx:id="quizlet" layoutX="34.0" layoutY="151.0" onAction="#onQuizlet" text="Quizlet Set" textFill="WHITE" toggleGroup="$studyPick" />
            <JFXTextField fx:id="afSetEnter" focusColor="BLACK" layoutX="78.0" layoutY="138.0" promptText="Set Name" style="-fx-background-color: #FFFFFF70;" unFocusColor="#616161" />
            <Button fx:id="afSetAdd" layoutX="228.0" layoutY="138.0" mnemonicParsing="false" onAction="#afSetAdd" style="-fx-background-color: #303030;" text="Add" textFill="WHITE" />
            <Label layoutX="73.0" layoutY="28.0" text="New Study Set" textFill="#fffafa">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
     </center>
</BorderPane>
