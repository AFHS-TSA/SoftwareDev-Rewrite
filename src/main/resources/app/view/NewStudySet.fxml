<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="baseBorder" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.app.controllers.NewStudySetController">
	 <center>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <JFXRadioButton fx:id="self" layoutX="89.0" layoutY="88.0" onAction="#onSelf" selected="true" text="Self Made Set">
               <toggleGroup>
                  <ToggleGroup fx:id="studyPick" />
               </toggleGroup>
            </JFXRadioButton>
            <JFXRadioButton fx:id="quizlet" layoutX="89.0" layoutY="119.0" onAction="#onQuizlet" text="Quizlet Set" toggleGroup="$studyPick" />
            <JFXTextField fx:id="afSetEnter" layoutX="42.0" layoutY="115.0" />
            <Button fx:id="afSetAdd" layoutX="201.0" layoutY="115.0" mnemonicParsing="false" onAction="#afSetAdd" style="-fx-background-color: #303030;" text="Add" textFill="WHITE" />
         </children>
      </AnchorPane>
     </center>
</BorderPane>
